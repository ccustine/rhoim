# HuggingFace Token Secret Template
# Copy this file to secret.yaml and add your actual token
# DO NOT commit secret.yaml to version control

apiVersion: v1
kind: Secret
metadata:
  name: huggingface-token
  labels:
    app: rhoim
type: Opaque
stringData:
  # Replace with your actual HuggingFace token
  # Get token from: https://huggingface.co/settings/tokens
  token: "hf_REPLACE_WITH_YOUR_TOKEN"
